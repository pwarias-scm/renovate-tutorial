{ 
"enabledManagers": ["regex"],
"packageRules": [
  {
      "matchDatasources": ["docker"]
    },
    {
      "enabled": true,
      "matchManagers": ["regex"],
      "matchPackagePrefixes": ["lodash"],
      "automerge": true,
      "labels": ["renovate", "Review-Bypass"],
      "prBodyNotes": [
        "This looks like a PR auto-generated by Renovate (tagged with 'renovate').",
        "If this is a change to an image version, please review the image's corresponding changelog here:",
          "- lodash: https://github.com/",
          "- example: https://github.com/",
          "- example2: https://github.com/",
          "- example3: https://github.com/",
          "- example4: https://github.com/"
      ]
    }
  ],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["package.json"],
      "matchStrings": [
        "\"(?<depName>lodash)\":\\s*\"(?<currentValue>\\d+\\.\\d+\\.\\d+)\""
      ],
      "datasourceTemplate": "npm"
    }
  ]
}
